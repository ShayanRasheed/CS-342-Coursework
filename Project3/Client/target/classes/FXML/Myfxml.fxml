<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>

<?import javafx.scene.text.Text?>
<VBox xmlns:fx="http://javafx.com/fxml/1" styleClass="vbox" fx:controller = "MyController"
	  fx:id = "root" spacing="50" alignment="CENTER">

	<Text text="The Ancient Game of Morra"/>
	<Text text="Enter and IP address and a Port to connect to:"/>
    <TextField fx:id = "IP" promptText="Enter IP address..."/>
	<TextField fx:id = "port" promptText="Enter port number..."/>
	<Button fx:id = "start" text = "Start Game!" onAction="#createClient"/>
	
</VBox>

