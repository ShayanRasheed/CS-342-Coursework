<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>

<?import javafx.scene.layout.BorderPane?>

<?import javafx.scene.text.Text?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1"
			fx:id = "root" styleClass="border" fx:controller = "MyController">
	<top>
		<VBox alignment="CENTER">
			<Text fx:id="title" text="Homework Five"/>
		</VBox>
	</top>

	<left>
		<VBox spacing="20">
			<Button fx:id = "b1" text = "button 1" onAction="#b1Method"/>
			<Button fx:id = "b2" text="button 2" onAction="#b2Method"/>
		</VBox>
	</left>

	<center><TextField fx:id="input" promptText="enter text here then press button 1"/></center>

	<right><TextField fx:id ="output" text="final string goes here" editable="false" prefWidth="150"/></right>
</BorderPane>



